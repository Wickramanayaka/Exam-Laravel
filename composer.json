{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.5",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "picqer/php-barcode-generator": "^2.2",
        "proactiveants/admin": "~1.0-dev",
        "proactiveants/advertisement": "~1.0-dev",
        "proactiveants/cashier": "1.0.x-dev",
        "proactiveants/course": "~1.0-dev",
        "proactiveants/exam": "~1.0-dev",
        "proactiveants/lesson": "~1.0-dev",
        "proactiveants/library": "~1.0-dev",
        "proactiveants/seminar": "~1.0-dev",
        "proactiveants/store": "~1.0-dev",
        "proactiveants/teacher": "^1.0@dev",
        "simplesoftwareio/simple-qrcode": "^4.2"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/proactiveants/library",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/admin",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/seminar",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/lesson",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/store",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/exam",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/course",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/advertisement",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/teacher",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "packages/proactiveants/cashier",
            "options": {
                "symlink": true
            }
        }
        
    ],
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
